{
  "name": "days-between-two-dates-utility",
  "version": "1.0.2",
  "description": "Identify weekdays and business days between two dates.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify",
    "compile": "tsc",
    "start": "nodemon ./src/index.ts",
    "start:prod": "node ./dist/index.js",
    "test": "jest --coverage",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janhavi529/ts-days-between-two-dates.git"
  },
  "author": "Janhavi Toshkhani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/janhavi529/ts-days-between-two-dates/issues"
  },
  "homepage": "https://github.com/janhavi529/ts-days-between-two-dates#readme",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "app-root-path": "^3.1.0"
  }
}
